{
    "global-object-type": "MediaCompound",
    "ui-type-name": "Media Compound",
    "storage-type": "WS",
    "storage-object-type": "KBaseBiochem.Media",
    "versions": [{
        "inner-sub-type": "MediaCompound",
        "path-to-sub-objects": "/mediacompounds/*/",
        "primary-key-path": "id",
        "indexing-rules": [
            {
                "path": "id",
                "keyword-type": "string",
                "ui-name": "ID"
            },
            {
                "path": "name",
                "full-text": true
            },
            {
                "path": "inchikey",
                "keyword-type": "string",
                "ui-name": "InChIKey"
            },
            {
                "path": "concentration",
                "keyword-type": "float",
                "ui-name": "Concentration"
            },
            {
                "path": "minFlux",
                "keyword-type": "float",
                "ui-name": "Minimum Flux"
            },
            {
                "path": "maxFlux",
                "keyword-type": "float",
                "ui-name": "Maximum Flux"
            },
        ]
    }]
}